  "type": "object",
  "required": [
    "base_denom",
    "mint_contract",
    "oracle_contract",
    "premium_min_update_interval",
    "terraswap_factory"
  ],
  "properties": {
    "base_denom": {
      "type": "string"
    },
    "mint_contract": {
      "type": "string"
    },
    "oracle_contract": {
      "type": "string"
    },
    "premium_min_update_interval": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "terraswap_factory": {
      "type": "string"
    }
  }
}
