{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "PoolInfoResponse",
  "type": "object",
  "required": [
    "asset_token",
    "pending_reward",
    "premium_rate",
    "premium_updated_time",
    "reward_index",
    "short_pending_reward",
    "short_reward_index",
    "staking_token",
    "total_bond_amount",
    "total_short_amount"
  ],
  "properties": {
    "asset_token": {
      "$ref": "#/definitions/HumanAddr"
    },
    "pending_reward": {
      "$ref": "#/definitions/Uint128"
    },
    "premium_rate": {
      "$ref": "#/definitions/Decimal"
    },
    "premium_updated_time": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "reward_index": {
      "$ref": "#/definitions/Decimal"
    },
    "short_pending_reward": {
      "$ref": "#/definitions/Uint128"
    },
    "short_reward_index": {
      "$ref": "#/definitions/Decimal"
    },
    "staking_token": {
      "$ref": "#/definitions/HumanAddr"
    },
    "total_bond_amount": {
      "$ref": "#/definitions/Uint128"
    },
    "total_short_amount": {
      "$ref": "#/definitions/Uint128"
    }
  },
  "definitions": {
    "Decimal": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal(1_000_000_000_000_000_000) == 1.0 The greatest possible value that can be represented is 340282366920938463463.374607431768211455 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "HumanAddr": {
      "type": "string"
    },
    "Uint128": {
      "type": "string"
    }
  }
}
